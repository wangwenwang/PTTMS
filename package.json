    "react": "16.0.0-alpha.6",
    "react-dom": "^15.5.4",
    "react-native": "0.43.3",
    "react-native-actionsheet": "^2.1.0",
    "react-native-baidu-map": "^0.6.0",
    "react-native-camera": "git+https://github.com/lwansbrough/react-native-camera.git",
    "react-native-image-crop-picker": "^0.13.1",
